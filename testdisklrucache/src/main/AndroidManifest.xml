<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zz.com.hanhan.testdisklrucache">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="zz.com.hanhan.testdisklrucache.custom.SimpleGlideModule"
            android:value="GlideModule" />

        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/translucent" />
        <activity android:name=".activities.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.LruCacheActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".activities.PictureSelectActivity" />
        <activity android:name=".activities.ListviewDeleteActivity"/>
        <activity android:name=".activities.DragPictureActivity"
            android:theme="@style/BaseAppTheme.Trans"/>
    </application>

</manifest>